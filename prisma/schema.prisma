generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Gola {
  poloCodigo String @id
  quantidade Int
  Polo       Polo   @relation(fields: [poloCodigo], references: [codigo])
}

model Polo {
  codigo String @id
  cor    String
  Gola   Gola?
  Punho  Punho?
}

model Punho {
  poloCodigo String @id
  quantidade Int
  Polo       Polo   @relation(fields: [poloCodigo], references: [codigo])
}

model usuarios {
  id     Int    @id @default(autoincrement())
  nome   String @unique
  senha  String @default("12345")
  Eadmin Int
}
